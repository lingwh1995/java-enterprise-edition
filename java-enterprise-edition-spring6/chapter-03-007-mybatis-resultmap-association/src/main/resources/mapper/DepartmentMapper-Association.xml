<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bluebridge.dao.IDepartmentDao">
	<!-- 根据deptNo获取Department -->
	<select id="getDeptByDeptNo"  resultType="org.bluebridge.domain.Department">
		select * from t_department where dept_no = #{deptNo}
	</select>
</mapper>